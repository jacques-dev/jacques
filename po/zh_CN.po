#: ../src/main.c:85
#, c-format
msgid ""
"error: unknown command '%s'\n"
"\n"
msgstr ""
"错误: 无法识别的命令 '%s'\n"
"\n"

#: ../src/main.c:94 ../src/main.c:112
#, c-format
msgid "%s version: %s/%s\n"
msgstr "%s 版本: %s/%s\n"

#: ../src/main.c:96
#, c-format
msgid ""
"Usage: %s start|stop|check [options]\n"
"\n"
msgstr ""
"使用方法: %s start|stop|check [选项]\n"
"\n"

#: ../src/main.c:97
#, c-format
msgid "Command:\n"
msgstr "命令:\n"

#: ../src/main.c:98
#, c-format
msgid "  start\t: start jacques daemon\n"
msgstr "  start\t: 启动jacques\n"

#: ../src/main.c:99
#, c-format
msgid "  stop\t: stop jacques daemon and all server processes\n"
msgstr "  stop\t: 停止jacques服务\n"

#: ../src/main.c:101
#, c-format
msgid "  check\t: check to see if the runtime environment is OK\n"
msgstr "  check\t: 检查jacques的运行环境是否可行\n"

#: ../src/main.c:103
#, c-format
msgid "Options:\n"
msgstr "选项:\n"

#: ../src/main.c:104
#, c-format
msgid "  -h\t: show this help\n"
msgstr "  -h\t: 显示本帮助\n"

#: ../src/main.c:105
#, c-format
msgid "  -v\t: show version and exit\n"
msgstr "  -v\t: 显示版本号并退出\n"

#: ../src/main.c:128
#, c-format
msgid "jacques is already running!\n"
msgstr "jacques已在运行!\n"

